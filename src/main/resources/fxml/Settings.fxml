            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.g3_dev.Settings">
    <children>
        <Label layoutX="14.0" layoutY="54.0" prefHeight="26.0" prefWidth="162.0" text="%lbl_tweetsLimit"
               AnchorPane.bottomAnchor="77.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
        <Button fx:id="btnSaveSettings" layoutX="283.0" layoutY="82.0" mnemonicParsing="false" onAction="#saveSettings"
                styleClass="twitter_blue" text="%btn_save" textFill="WHITE" AnchorPane.bottomAnchor="14.0"
                AnchorPane.rightAnchor="14.0"/>
        <Spinner fx:id="spnTweetLimit" layoutX="15.0" layoutY="81.0" prefWidth="200.0" AnchorPane.bottomAnchor="50.0"
                 AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
        <Label layoutX="24.0" layoutY="51.0" prefHeight="26.0" prefWidth="162.0" text="%lbl_tweetLang"
               AnchorPane.bottomAnchor="136.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
        <ComboBox fx:id="cbTweetsLang" layoutX="114.0" layoutY="42.0" prefWidth="200.0" AnchorPane.bottomAnchor="109.0"
                  AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
        <ComboBox fx:id="cbLang" layoutX="24.0" layoutY="80.0" prefWidth="200.0" AnchorPane.bottomAnchor="168.0"
                  AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
        <Label layoutX="24.0" layoutY="184.0" prefHeight="26.0" prefWidth="162.0" text="%lbl_appLang"
               AnchorPane.bottomAnchor="195.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
        <Button layoutX="14.0" layoutY="193.0" mnemonicParsing="false" onAction="#openChangeKeys"
                text="%btn_changeKeys" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0"/>
    </children>
</AnchorPane>
